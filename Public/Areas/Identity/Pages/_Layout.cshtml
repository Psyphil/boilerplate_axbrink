    <script src="/node_modules/perfect-scrollbar/dist/perfect-scrollbar.min.js"></script>
    <script src="/node_modules/@Html.Raw("@")coreui/coreui/dist/js/coreui.min.js"></script>

    <partial name="_ValidationScriptsPartial" />

</body>
</html>