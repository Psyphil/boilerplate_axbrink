    <!-- Icons-->
    <link href="/node_modules/@Html.Raw("@")coreui/icons/css/coreui-icons.min.css" rel="stylesheet">
    <link href="/node_modules/flag-icon-css/css/flag-icon.min.css" rel="stylesheet">
